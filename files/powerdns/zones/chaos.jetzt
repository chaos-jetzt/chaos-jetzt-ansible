$ORIGIN chaos.jetzt.
@ 3600 IN SOA ns.chaos.jetzt. administration.chaos.jetzt.  (
    2020091401 ; serial ;;; UPDATE THIS AFTER EACH EDIT!
    3600       ; refresh (1 hour)
    1800       ; retry (30 min)
    604800     ; expire (168 days)
    3600       ; minimum (1 hour)
)

@ 3600 IN NS ns1.he.net.
@ 3600 IN NS ns2.he.net.
@ 3600 IN NS ns3.he.net.
@ 3600 IN NS ns4.he.net.
@ 3600 IN NS ns5.he.net.

ns IN A {{ ansible_default_ipv4.address }}
ns IN AAAA {{ ansible_default_ipv6.address }}

;; MAIL
;;;; MX
@ 3600 IN MX 20 mxext3.mailbox.org.
@ 3600 IN MX 10 mxext1.mailbox.org.
@ 3600 IN MX 10 mxext2.mailbox.org.

;;;; SPF
@ 3600 IN TXT "v=spf1 include:mailbox.org ~all"

;;;; DKIM
MBO0001._domainkey 3600 IN TXT "v=DKIM1; k=rsa; "
MBO0002._domainkey 3600 IN TXT "v=DKIM1; k=rsa; "

;; TXT - Verify
_github-challenge-chaos-jetzt.chaos.jetzt. 3600 IN TXT "db12da3d46"
{{ lookup('passwordstore', 'infra/dns/mailbox.org subkey=record') }} 3600 IN TXT "{{ lookup('passwordstore', 'infra/dns/mailbox.org') }}"

;; Pages
